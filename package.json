{
  "name": "esbuild-lambda",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib",
    "bundle": "npm run build && serverless package",
    "postbundle": "del-cli .serverless/*.json",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Tramaine L. Gillus",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "18.4.1",
    "@commitlint/config-conventional": "18.4.0",
    "@types/node": "20.9.0",
    "@types/oracledb": "6.0.3",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "del-cli": "5.1.0",
    "eslint": "8.53.0",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "serverless": "3.36.0",
    "serverless-esbuild": "1.48.5",
    "serverless-offline": "13.2.1",
    "tsx": "4.1.2",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "dayjs": "1.11.10",
    "oracledb": "6.2.0"
  }
}
